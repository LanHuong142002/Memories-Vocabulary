{
  "name": "practice-four",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint:eslint": "eslint --ignore-path .eslintignore . --ext .js,.jsx,.ts,.tsx",
    "fix:eslint": "eslint --fix --ignore-path .eslintignore . --ext .js,.jsx,.ts,.tsx",
    "jest": "jest --passWithNoTests",
    "jest:watch": "jest --watch",
    "prepare": "cd ../.. && husky install",
    "pre-commit-lint": "pnpm lint-staged",
    "pre-push-lint": "rm -rf dist/ && vite build"
  },
  "dependencies": {
    "axios": "1.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.12.1",
    "vite-plugin-environment": "1.1.3",
    "vite-tsconfig-paths": "4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@storybook/addon-essentials": "7.1.1",
    "@storybook/addon-interactions": "7.1.1",
    "@storybook/addon-links": "7.1.1",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.1.1",
    "@storybook/react": "7.1.1",
    "@storybook/react-vite": "7.1.1",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/dom": "8.20.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.4.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/testing-library__jest-dom": "5.14.6",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "@vitejs/plugin-react": "3.1.0",
    "eslint": "8.45.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-storybook": "0.6.11",
    "husky": "8.0.3",
    "jest": "29.1.2",
    "jest-environment-jsdom": "28.1.3",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "13.2.2",
    "prettier": "2.8.3",
    "storybook": "7.1.1",
    "ts-jest": "28.0.7",
    "typescript": "5.0.2",
    "vite": "4.4.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.{js,jsx,ts,tsx} --fix-dry-run --no-error-on-unmatched-pattern",
      "pnpm run jest"
    ]
  }
}
